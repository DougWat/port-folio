      <script type="text/javascript">
        var images = $(".showcase-item .wrapper img");
        $(images).each(function(i){
          $(this).closest(".wrapper").mouseenter(function(){
            if($(this).data("animating") != "true"){
              $(this).data("animating","true");
              loopCycle($(this).find("img"));
            }
          });
        });

        function loopCycle(el){
          var value = -$(el).height() + $(el).closest(".wrapper").height();
          var animationtime = $(el).height() / 1000;

          $(el).animate({
            top: value
          },3000 * animationtime,function(){
            $(el).animate({
              top:0
            },3000 * animationtime,function(){
              var wrapper = $(this).closest(".wrapper");

              if($(wrapper).is(":hover")){
                loopCycle(el);
              }
              else{
                $(wrapper).data("animating", "false");
              }
            });
          });
        }
      </script>
