    <script type="text/javascript">
      var quote = $(".quote");
      var quotetext = $(".quote h3");
      var patientname = $(".quote h4");

      function Quote(q, n){
        this.quote = q;
        this.name = n;
      }

      var quoteholder = [];
      quoteholder.push(new Quote("Lyrathol made everything in my life tolerable. I forgot how to worry.", "-Jimmin, Lyrathol patient"));
      quoteholder.push(new Quote("Ever since Lyrathol I don't remember why I was so scared of the corner of my room.","-Bella, Lyrathol patient"));
      quoteholder.push(new Quote("Because of Lyrathol I need not remember all that I see with my lidless eyes.", "-Lidless Joe, Lyrathol patient"));
      quoteholder.push(new Quote("I found something weird in my backyard, so my doctor recommended Lyrathol.", "-David, Lyrathol patient"));
      quoteholder.push(new Quote("I'm perfect because all of my mistakes are gone.", "-Kate, Lyrathol patient"));

      setInterval(function(){
        ChangeQuote();
      },8000);
      function ChangeQuote(){
        var rand;
        while(true){
          rand = Math.floor(Math.random() * quoteholder.length);
          if(quotetext.text !== ("<span>\"</span>" + quoteholder[rand].quote + "<span>\"</span>")){
            break;
          }
        }
        quote.fadeTo(500,0,function(){
          quotetext.html("<span>\"</span>" + quoteholder[rand].quote + "<span>\"</span>");
          patientname.html(quoteholder[rand].name);
          quote.fadeTo(500,1);
        });
      }

    </script>
